{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm install && npx vite build --outDir dist",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
